msgctxt "Name"
msgid "KTorrent"
msgstr "KTorrent"

#: ktorrent/ktorrent.desktop:60
msgctxt "GenericName"
msgid "BitTorrent Client"
msgstr "Client BitTorrent"

#: ktorrent/ktorrent.desktop:125
msgctxt "Comment"
msgid "A BitTorrent program for KDE"
msgstr "Client BitTorrent pentru KDE"

#: ktorrent/ktorrent.notifyrc:3
msgctxt "Comment"
msgid "KTorrent"
msgstr "KTorrent"

#: ktorrent/ktorrent.notifyrc:60
msgctxt "Name"
msgid "Torrent silently opened"
msgstr "Torent deschis silențios"

#: ktorrent/ktorrent.notifyrc:103
msgctxt "Name"
msgid "Torrent stopped by error"
msgstr "Torentul a fost oprit de o eroare"

#: ktorrent/ktorrent.notifyrc:156
msgctxt "Name"
msgid "Torrent has finished downloading"
msgstr "Descărcarea torentului s-a încheiat"

#: ktorrent/ktorrent.notifyrc:209
msgctxt "Name"
msgid "Maximum share ratio reached"
msgstr "A fost atinsă rata maximă de partajare"

#: ktorrent/ktorrent.notifyrc:262
msgctxt "Name"
msgid "Maximum seed time reached"
msgstr "A fost atinsă durata maximă de încărcare"

#: ktorrent/ktorrent.notifyrc:315
msgctxt "Name"
msgid "Disk space is running low"
msgstr "Spațiul pe disc se termină"

#: ktorrent/ktorrent.notifyrc:368
msgctxt "Name"
msgid "Corrupted data has been found"
msgstr "Au fost găsite date corupte"

#: ktorrent/ktorrent.notifyrc:422
msgctxt "Name"
msgid "Torrent cannot be enqueued"
msgstr "Torentul nu poate fi pus în coadă"

#: ktorrent/ktorrent.notifyrc:475
msgctxt "Name"
msgid "Torrent cannot be started"
msgstr "Torentul nu poate fi pornit"

#: ktorrent/ktorrent.notifyrc:528
msgctxt "Name"
msgid "Torrent cannot be loaded silently"
msgstr "Torentul nu poate fi încărcat silențios"

#: ktorrent/ktorrent.notifyrc:581
msgctxt "Name"
msgid "DHT is not enabled"
msgstr "DHT nu este activat"

#: ktorrent/ktorrent.notifyrc:629
msgctxt "Name"
msgid "Event generated by plugin"
msgstr "Eveniment generat de extensie"

#: ktorrent/ktorrent.notifyrc:674
msgctxt "Name"
msgid "Magnet link download started"
msgstr "Descărcarea legăturii magnet a început"

#: ktorrent/ktorrentplugin.desktop:4
msgctxt "Name"
msgid "KTorrent Plugin"
msgstr "Modul KTorrent"

#: plasma/applet/plasma-applet-ktorrent.desktop:62
msgctxt "Comment"
msgid "Plasma widget to keep track of a single torrent"
msgstr "Plasmoid pentru evidența unui singur torent"

#: plasma/dataengine/plasma-dataengine-ktorrent.desktop:60
msgctxt "Comment"
msgid "KTorrent data engine, for getting information from KTorrent"
msgstr "Motor de date KTorrent, pentru obținerea informațiilor de la KTorrent"

#: plugins/bwscheduler/ktbwschedulerplugin.desktop:2
msgctxt "Name"
msgid "Scheduler"
msgstr "Programare"

#: plugins/bwscheduler/ktbwschedulerplugin.desktop:48
msgctxt "Comment"
msgid "Schedule upload and download limits over a period of a week"
msgstr ""
"Planifică limitele de încărcare și descărcare pe perioada unei săptămîni"

#: plugins/downloadorder/ktdownloadorderplugin.desktop:2
msgctxt "Name"
msgid "Download Order"
msgstr "Ordinea descărcării"

#: plugins/downloadorder/ktdownloadorderplugin.desktop:52
msgctxt "Comment"
msgid "Specify the download order of a multi-file torrent"
msgstr "Specifică ordinea de descărcare a unui torent multifișier"

#: plugins/infowidget/ktinfowidgetplugin.desktop:2
msgctxt "Name"
msgid "Information Widget"
msgstr "Control grafic informativ"

#: plugins/infowidget/ktinfowidgetplugin.desktop:51
msgctxt "Comment"
msgid "Displays general information about a torrent in several tabs"
msgstr "Afișează informații generale despre un torent în cîteva file"

#: plugins/ipfilter/ktipfilterplugin.desktop:2
msgctxt "Name"
msgid "IP Filter"
msgstr "Filtru IP"

#: plugins/ipfilter/ktipfilterplugin.desktop:54
msgctxt "Comment"
msgid "Filter IP addresses through a blocklist"
msgstr "Filtrează adresele IP printr-o listă de blocare"

#: plugins/logviewer/ktlogviewerplugin.desktop:2
msgctxt "Name"
msgid "Log Viewer"
msgstr "Vizualizator jurnal"

#: plugins/logviewer/ktlogviewerplugin.desktop:55
msgctxt "Comment"
msgid "Displays the logging output"
msgstr "Afișează înregistrările din jurnal"

#: plugins/magnetgenerator/ktmagnetgeneratorplugin.desktop:2
msgctxt "Name"
msgid "Magnet Generator"
msgstr "Generator Magnet"

#: plugins/magnetgenerator/ktmagnetgeneratorplugin.desktop:46
msgctxt "Comment"
msgid "Generates magnet links"
msgstr "Generează legături Magnet"

#: plugins/mediaplayer/ktmediaplayerplugin.desktop:2
msgctxt "Name"
msgid "Media Player"
msgstr "Redare multimedia"

#: plugins/mediaplayer/ktmediaplayerplugin.desktop:56
msgctxt "Comment"
msgid "Phonon-based media player"
msgstr "Lector multimedia bazat pe Phonon"

#: plugins/scanfolder/ktscanfolderplugin.desktop:2
msgctxt "Name"
msgid "Scan Folder"
msgstr "Citește dosar"

#: plugins/scanfolder/ktscanfolderplugin.desktop:54
msgctxt "Comment"
msgid "Scan folders for torrent files and load them"
msgstr "Citește dosare pentru fișiere-torent și le încarcă"

#: plugins/scripting/ktscriptingplugin.desktop:2
msgctxt "Name"
msgid "Scripting"
msgstr "Scriptare"

#: plugins/scripting/ktscriptingplugin.desktop:53
msgctxt "Comment"
msgid "Enables Kross scripting support"
msgstr "Activează suportul pentru scripturi Kross"

#: plugins/scripting/scripts/auto_remove/auto_remove.desktop:2
msgctxt "Name"
msgid "Automatic Remove"
msgstr "Eliminare automată"

#: plugins/scripting/scripts/auto_remove/auto_remove.desktop:52
msgctxt "Comment"
msgid ""
"Script to automatically remove torrents when they finish downloading or "
"seeding"
msgstr ""
"Script pentru eliminarea automată a torentelor la încheierea descărcării sau "
"încărcării acestora"

#: plugins/scripting/scripts/auto_resume/auto_resume.desktop:2
msgctxt "Name"
msgid "Automatic Resume"
msgstr "Reluare automată"

#: plugins/scripting/scripts/auto_resume/auto_resume.desktop:53
msgctxt "Comment"
msgid ""
"Script to automatically resume after some time, when the suspended state is "
"enabled"
msgstr ""
"Script pentru reluarea automată după un timp, cînd e activată starea "
"suspendată"

#: plugins/scripting/scripts/email_notifications/email_notifications.desktop:2
msgctxt "Name"
msgid "E-Mail Notifications"
msgstr "Notificări prin poștă"

#: plugins/scripting/scripts/email_notifications/email_notifications.desktop:53
msgctxt "Comment"
msgid "Script to notify you of events via e-mail"
msgstr "Script ce vă notifică despre evenimente prin poșta electronică"

#: plugins/scripting/scripts/tracker_groups/tracker_groups.desktop:2
msgctxt "Name"
msgid "Tracker Grouping"
msgstr "Grupare Tracker"

#: plugins/scripting/scripts/tracker_groups/tracker_groups.desktop:52
msgctxt "Comment"
msgid "Script to create automatic groups based upon tracker URLs"
msgstr ""
"Script pentru crearea grupurilor automate pe baza URL-urilor tracker-ului"

#: plugins/search/ktsearchplugin.desktop:2
msgctxt "Name"
msgid "Search"
msgstr "Caută"

#: plugins/search/ktsearchplugin.desktop:55
msgctxt "Comment"
msgid "Search for torrents"
msgstr "Caută torente"

#: plugins/shutdown/ktshutdownplugin.desktop:2
msgctxt "Name"
msgid "Shutdown"
msgstr "Oprire"

#: plugins/shutdown/ktshutdownplugin.desktop:56
msgctxt "Comment"
msgid "Allows you to shutdown your computer when torrents finish"
msgstr "Vă permite să opriți calculatorul la încheierea torentelor"

#: plugins/stats/ktstatsplugin.desktop:3
msgctxt "Name"
msgid "Statistics"
msgstr "Statistici"

#: plugins/stats/ktstatsplugin.desktop:57
msgctxt "Comment"
msgid "Shows statistics about torrents in several graphs"
msgstr "Afișează statistici despre torente în diverse grafice"

#: plugins/syndication/ktsyndicationplugin.desktop:2
msgctxt "Name"
msgid "Syndication"
msgstr "Redifuzare"

#: plugins/syndication/ktsyndicationplugin.desktop:51
msgctxt "Comment"
msgid "Syndication plugin for KTorrent, supporting RSS and Atom feeds"
msgstr "Modul de redifuzare pentru KTorrent, ce susține fluxuri RSS și Atom"

#: plugins/upnp/ktupnpplugin.desktop:2
msgctxt "Name"
msgid "UPnP"
msgstr "UPnP"

#: plugins/upnp/ktupnpplugin.desktop:55
msgctxt "Comment"
msgid "Forward ports using UPnP"
msgstr "Înaintarea porturilor utilizînd UPnP"

#: plugins/webinterface/ktwebinterfaceplugin.desktop:2
msgctxt "Name"
msgid "Web Interface"
msgstr "Interfață web"

#: plugins/webinterface/ktwebinterfaceplugin.desktop:56
msgctxt "Comment"
msgid "Allows control of KTorrent via a web interface"
msgstr "Permite controlul KTorrent printr-o interfață web"

#: plugins/zeroconf/ktzeroconfplugin.desktop:2
msgctxt "Name"
msgid "Zeroconf"
msgstr "ZeroConf"

#: plugins/zeroconf/ktzeroconfplugin.desktop:54
msgctxt "Comment"
msgid "Discover peers on the local network using the Zeroconf protocol"
msgstr ""
"Descoperirea partenerilor în rețeaua locală utilizînd protocolul Zeroconf"

