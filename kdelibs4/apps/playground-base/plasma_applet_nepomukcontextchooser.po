msgid "Current work context: <emphasis>%1</emphasis>"
msgstr "Contextul de lucru curent: <emphasis>%1</emphasis>"

#: nepomukcontextchooser.cpp:103
#, kde-format
msgctxt "@info:tooltip"
msgid ""
"You are working in the context of <emphasis>%1</emphasis>. This means that "
"files, emails, and other resources can easily be related to it."
msgstr ""
"Lucrați în contextul <emphasis>%1</emphasis>. Aceasta înseamnă că fișierele, "
"scrisorile și alte resurse pot fi legate de acesta cu ușurință."

#: nepomukcontextchooser.cpp:110
msgctxt "@action:button"
msgid "No work context selected"
msgstr "Niciun context de lucru ales"

#: nepomukcontextchooser.cpp:113
msgctxt "@info:tooltip"
msgid "No work context selected"
msgstr "Niciun context de lucru ales"

#: nepomukcontextchooser.cpp:115
msgctxt "@info:tooltip"
msgid ""
"Choose the current work context to be able to easily relate files, emails, "
"and other resources to it."
msgstr ""
"Alegeți contextul de lucru actual pentru a putea face legătura ușor între "
"acesta și fișiere, scrisori și alte resurse."

#: nepomukcontextchooser.cpp:134
